{
    "compilerOptions": {
        "target": "ES2019",
        "module": "CommonJS",
        "moduleResolution": "node",
        "resolveJsonModule": true,
        "esModuleInterop": true,
        "suppressExcessPropertyErrors": true,
        "types": [
            "node",
            "webdriverio/async",
            "@wdio/globals/types",
            "@wdio/cucumber-framework",
            "expect-webdriverio",
            "@wdio/selenium-standalone-service"
        ]
    }
}
